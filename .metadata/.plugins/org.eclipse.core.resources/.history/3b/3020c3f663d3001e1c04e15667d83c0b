#ifndef BASE_H_
#define BASE_H_

#define PERIPHERAL_BASE                     (0x4000 0000)

#define APB_PERIPHERAL_OFFSET               (0x0000)
#define APB_PERIPHERAL                      (*(volatile unsigned int *)PERIPHERAL_BASE + APB_PERIPHERAL_OFFSET)








#endif /* BASE_H_ */
